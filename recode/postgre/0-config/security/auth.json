{
  "users": {
    "model": "vw_usuario",
    "path": "usuario",
    "engine": "postgre",
    "fields": {
      "username": "correo",
      "password": "password",
      "id": "usuario_id",
      "email": "email",
      "profile": {
        "table": "group",
        "key": "profile"
      }
    },
    "addFields": {
      "fullName": "capitalize(this.usuario)",
      "getID": "this.usuario_id",
      "isClient": "false",
      "path": "'usuario'",
      "menus": "(this.tipo_institucion==ENUM_2.COMPANY_TYPE.publica?'menus':'menus_privado')"
    },
    "hash": "md5",
    "autenticacion": "database",
    "ADuser": "",
    "ADpassword": "",
    "ADpath": "LDAP://DIRECCION_IP/DC=DOMINIO,DC=local",
    "expire": "180",
    "attempts": 4,
    "unlockTime": 3,
    "super": [
      "admin@eisdr.com"
    ]
  },
  "audit": {
    "insert": {
      "created_by": "new SESSION().current().getID()",
      "created_at": "new date().now()",
      "compania": "new SESSION().current().compania_id",
      "institucion": "new SESSION().current().institucion_id"
    },
    "update": {
      "updated_by": "new SESSION().current().getID()",
      "updated_at": "new date().now()",
      "compania": "new SESSION().current().compania_id",
      "institucion": "new SESSION().current().institucion_id"
    },
    "delete": {
      "deleted_by": "new SESSION().current().getID()",
      "deleted_at": "new date().now()",
      "compania": "new SESSION().current().compania_id",
      "institucion": "new SESSION().current().institucion_id"
    }
  }
}
